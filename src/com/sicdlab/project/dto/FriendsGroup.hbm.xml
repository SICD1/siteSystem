<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlab.project.dto">
    <class name="FriendsGroup" table="tb_friends_group">
        <id name="friendsGroupID" type="java.lang.String" column="friendsGroupID">
            <generator class="uuid"/>
        </id>
        <property name="friendsGroupName" type="java.lang.String" column="friendsGroupName" />
    </class>
</hibernate-mapping>