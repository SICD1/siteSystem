<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlab.project.dto">
    <class name="Leave" table="tb_leave">
        <id name="leaveID" type="java.lang.String" column="leaveid">
            <generator class="uuid"/>
        </id>
        <property name="leaveContent" type="java.lang.String" column="leave_content" />
        <property name="leaveTime" type="java.util.Date" column="leave_time" />
        <property name="leaveShow" type="java.lang.Boolean" column="leave_show" />
    </class>
</hibernate-mapping>